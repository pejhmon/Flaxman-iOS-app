<root>
        <widget id="com.coolappz.HTML5Application" version="0.0.1">
        <name>UCL Art Museum</name>
        <description>
            Navigation app to discover artworks belonging to the Flaxman Art Gallery within UCL Campus.
        </description>
        <author email="zcahg73@ucl.ac.uk">
            MSc Computer Science Group 20
            Group Leader: Pejhmon Kamaie
        </author>
        <content src="index.html" />
        <access origin="*" />
        <!-- for google maps -->
<access origin="*.googleapis.com" />
<access origin="*.gstatic.com" />
<access origin="*.google.com" />
<access origin="maps.googleapis.com" />
<access origin="maps.gstatic.com" />
<access origin="mt0.googleapis.com" />
<access origin="mt1.googleapis.com" />
<access origin="csi.gstatic.com" />
<preference name="OpenAllWhitelistURLsInWebView" value="false" />
    </widget>
</root>
